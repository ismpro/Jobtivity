{
    "source": {
        "includePattern": ".+\\.js(doc|x)?$",
        "include": [
            "."
        ],
        "exclude": [
            "node_modules",
            "docs"
        ] 
    },
    "recurseDepth": 10, 
    "opts": {
        "destination": "./docs/jsdocs", 
        "recurse": true
    }
}